<h1>Edit Party</h1>

<div class="row">
  <div class="col-md-8">
    <%= form_for @party do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, class: 'form-control', placeholder: "Enter party title" %>
      </div>
      
      <%= f.submit "Save", class: 'btn btn-success' %>
    <% end %>
  </div>
</div>